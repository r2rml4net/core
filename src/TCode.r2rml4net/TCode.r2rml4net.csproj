<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net452</TargetFrameworks>
    <RootNamespace>TCode.r2rml4net</RootNamespace>
    <AssemblyName>TCode.r2rml4net</AssemblyName>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
    <GenerateAssemblyFileVersionAttribute>false</GenerateAssemblyFileVersionAttribute>
    <GenerateAssemblyInformationalVersionAttribute>false</GenerateAssemblyInformationalVersionAttribute>
  </PropertyGroup>
  <PropertyGroup>
    <Authors>Tomasz Pluskiewicz</Authors>
    <Owners>Tomasz Pluskiewicz</Owners>
    <Title>r2rml4net</Title>
    <Description>
      A C# implementation of R2RML, which allows creating R2RML and converting
      relational data to RDF using the .NET Framework
    </Description>
    <Copyright>Copyright 2012-2018 Tomasz Pluskiewicz</Copyright>
    <PackageTags>rdf r2rml semantic web</PackageTags>
    <PackageLicenseUrl>http://www.opensource.org/licenses/mit-license.php</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/r2rml4net/r2rml4net</PackageProjectUrl>
    <PackageIconUrl>http://lab.linkeddata.deri.ie/2011/rdf-logos/png/r2rml-128.png</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Mapping\Fluent\Queries\ReplaceShortcuts.rq" />
    <EmbeddedResource Include="DataSets\languages.ttl" />
    <Content Include="FodyWeavers.xml" />
    <EmbeddedResource Include="Validation\LanguageRegex.txt" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>